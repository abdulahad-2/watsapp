{
  "framework": "create-react-app",
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://watsapp-backend-mscv.onrender.com/api/$1"
    },
    {
      "source": "/ws",
      "destination": "https://watsapp-backend-mscv.onrender.com/"
    }
  ]
}
