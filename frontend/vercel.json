{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://watsapp-backend-mscv.onrender.com/api/$1"
    },
    {
      "source": "/ws/(.*)",
      "destination": "https://watsapp-backend-mscv.onrender.com/ws/$1"
    }
  ]
}
